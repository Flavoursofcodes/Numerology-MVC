      <div class="form-group">
      <label class="Language" for="inputFirstName">Language</label>
        <select name="lang" id="lang" required class="form-control">
          <option value="English" selected >English</option>
          <option value="Malay">Malay</option>
          <option value="Chinese">中文</option>
        </select>
      </div>
      <div class="form-group">
        <label class="First_name" for="inputFirstName">First name</label>
        <input name="name" type="text" class="form-control" id="firstname" placeholder="First name" required>
      </div>
      <div class="form-group">
        <label class="Last_name" for="inputLastName">Last name</label>
        <input name="lname" type="text" class="form-control" id="lastname" placeholder="Last name" required>
      </div>
      <div class="form-group">
        <label class="Date_of_Birth" for="inputDob">Date of Birth</label>
        <input type="text" name="dob" class="form-control" id="dob" onfocus="(this.type='date')" onblur="(this.type='text')" placeholder="DD-MM-YYYY" min="1000-01-01" max="2500-01-01" autocomplete="off"  required>
        <div class="alert alert-warning d-none" role="alert" id="dobWarning">
          Date format must be DD-MM-YYYY
        </div>
      </div>
      <div class="form-group">
        <label class="Time_of_Birth" for="inputDob">Time of Birth</label>
        <input type="time" name="timeofbirth" class="form-control" id="timeofbirth" placeholder="HH-MM" autocomplete="off"  required>
        <div class="alert alert-warning d-none" role="alert" id="timeWarning">
          Time format must be HH-MM in 24-hour notation.
        </div>
      </div>
        <div class="form-group">
        <label class="Gender" for="inputFirstName">Gender</label>
        <select name="gender" id="gender" required>
          <option class="select" value="" selected>select</option>
          <option class="male" value="Male" >Male</option>
          <option class="female" value="Female">Female</option>
        </select>
      </div>
      <div class="form-group">
        <label class="Twins_or_Triplets" for="inputFirstName">Twins or Triplets</label>
        <select name="twinsOrTriplets" id="twinsOrTriplets" required>
          <option class="select" value="" selected>select</option>
          <option class="no" value="No" >No</option>
          <option class="1st_twin" value="first_Twin">1st Twin</option>
          <option class="2nd_twin" value="second_Twin">2nd Twin</option>
          <option class="triplet" value="Triplet">Triplet</option>
        </select>
      </div>
      <div class="form-group d-none" id="fdobContainer"> <!-- dont remove d-none class from here -->
        <label class="Father_DOB" for="inputDob">Father DOB</label>
        <input type="date" name="fdob" class="form-control " id="fdob" autocomplete="off" min="1000-01-01" max="2500-01-01" placeholder="DD-MM-YYYY">
        <div class="alert alert-warning d-none" role="alert" id="fdobWarning">
          Date format must be DD-MM-YYYY
        </div>
      </div>
      <div class="form-group d-none" id="mdobContainer"> <!-- dont remove d-none class from here -->
        <label class="Mother_DOB" for="inputDob">Mother DOB</label>
        <input type="date" name="mdob" class="form-control  mdob"  id="mdob" autocomplete="off" min="1000-01-01" max="2500-01-01" placeholder="DD-MM-YYYY">
        <div class="alert alert-warning d-none" role="alert" id="mdobWarning">
          Date format must be DD-MM-YYYY
        </div>
      </div>
      <div class="form-group">
        <label class="Email_Address" for="inputEmail">Email Address</label>
        <input name="email" type="email" class="form-control" id="email" autocomplete="off" placeholder="Email Address" required>
      </div>
